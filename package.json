{
    "name": "goldcoders-cms",
    "version": "1.0.0",
    "description": "Goldcoders Corp CMS",
    "repository": "https://github.com/pitlords/site",
    "author": "Uriah Galang <ceo@goldcoders.dev>",
    "private": true,
    "license": "Commons Clause",
    "scripts": {
        "hugo": "hugo serve --disableFastRender",
        "netlify": "netlify dev",
        "lambda-serve": "netlify-lambda serve lambda",
        "lambda-build": "netlify-lambda build lambda",
        "lambda": "netlify-lambda",
        "postinstall": "netlify-lambda install",
        "cms-dev": "parcel cms/src/index.html -d cms/dist",
        "cms-build": "esbuild --bundle cms/src/main.ts --minify --outfile=static/admin/main.js",
        "build": "cross-env NODE_ENV=production node --require dotenv/config ./esbuild.js",
        "cms-git": "cross-env MODE=git netlify-cms-proxy-server",
        "cms-proxy": "netlify-cms-proxy-server",
        "dev": "run-p cms-proxy cms-dev netlify",
        "cms": "run-p hugo cms-proxy cms-dev",
        "local": "run-p cms-git cms-dev hugo"
    },
    "resolutions": {
    "node-forge": "0.10.0"
    },
    "devDependencies": {
        "@fullhuman/postcss-purgecss": "^2.1.2",
        "autoprefixer": "^9.7.6",
        "postcss": "^7.0.27",
        "postcss-cli": "^7.1.1",
        "postcss-import": "^12.0.1",
        "prettier": "2.0.5",
        "prettier-plugin-go-template": "0.0.4",
        "stylelint-config-recommended": "^3.0.0",
        "tailwindcss": "^1.9.3",
        "tailwindcss-aspect-ratio": "^3.0.0",
        "tailwindcss-responsive-embed": "^1.0.0"
    },
    "browserslist": [
        "last 1 version",
        "> 1%",
        "maintained node versions",
        "not dead"
    ],
    "dependencies": {
        "@tailwindcss/typography": "^0.2.0",
        "@tailwindcss/ui": "^0.6.2",
        "@types/node": "^14.11.5",
        "aws-sdk": "^2.686.0",
        "axios": "^0.19.2",
        "cross-env": "^7.0.2",
        "dotenv": "^8.2.0",
        "encoding": "^0.1.12",
        "esbuild": "^0.7.11",
        "google-spreadsheet": "^3.0.13",
        "js-yaml": "^3.14.0",
        "netlify-cms": "^2.10.61",
        "netlify-cms-proxy-server": "^1.3.6",
        "netlify-lambda": "^1.6.3",
        "npm-run-all": "^4.1.5",
        "parcel-bundler": "^1.12.4",
        "postcss-preset-env": "^6.7.0",
        "toml": "^3.0.0",
        "typescript": "^4.0.3"
    }
}

{
    "name": "goldcoders-cms",
    "version": "1.0.0",
    "description": "Goldcoders Corp CMS",
    "repository": "https://github.com/pitlords/site",
    "author": "Uriah Galang <ceo@goldcoders.dev>",
    "private": true,
    "license": "Commons Clause",
    "scripts": {
        "tw-dev": "TAILWIND_MODE=watch NODE_ENV=development postcss ./assets/css/main.css -c ./assets/css/tailwind.config.js -o ./assets/css/tailwind.css -w",
        "hugo-dev": "hugo serve --disableFastRender",
        "netlify": "netlify dev",
        "lambda-serve": "netlify-lambda serve lambda",
        "lambda-build": "netlify-lambda build lambda",
        "lambda": "netlify-lambda",
        "postinstall": "netlify-lambda install",
        "cms-dev": "parcel cms/src/index.html -d cms/dist",
        "cms-build": "esbuild --bundle cms/src/main.ts --minify --outfile=static/admin/main.js",
        "build-js": "cross-env NODE_ENV=production node --require dotenv/config ./esbuild.js",
        "tw-build": "TAILWIND_MODE=build NODE_ENV=production postcss ./assets/css/main.css -o ./assets/css/tailwind.css",
        "cms-git": "cross-env MODE=git netlify-cms-proxy-server",
        "cms-proxy": "netlify-cms-proxy-server",
        "dev": "run-p cms-proxy cms-dev netlify",
        "cms": "run-p hugo-dev cms-proxy cms-dev",
        "local": "run-p cms-git cms-dev hugo-dev",
        "build-all": "run-p tw-build hugo-build build-js",
        "watch": "run-p tw-dev hugo-dev",
        "build": "run-p tw-build hugo-build"
    },
    "resolutions": {
        "node-forge": "0.10.0"
    },
    "devDependencies": {
        "autoprefixer": "^10.2.6",
        "netlify-cli": "^3.35.1",
        "postcss": "^8.3.0",
        "postcss-cli": "^8.3.1",
        "postcss-import": "^12.0.1",
        "prettier": "2.0.5",
        "prettier-plugin-go-template": "0.0.4",
        "stylelint-config-recommended": "^3.0.0",
        "tailwindcss": "^2.1.4"
    },
    "browserslist": [
        "last 1 version",
        "> 1%",
        "maintained node versions",
        "not dead"
    ],
    "dependencies": {
        "@fullhuman/postcss-purgecss": "^4.0.3",
        "@tailwindcss/aspect-ratio": "^0.2.1",
        "@tailwindcss/forms": "^0.3.3",
        "@tailwindcss/typography": "^0.4.1",
        "@types/node": "^14.11.5",
        "aws-sdk": "^2.686.0",
        "axios": "^0.19.2",
        "cross-env": "^7.0.3",
        "dotenv": "^10.0.0",
        "encoding": "^0.1.12",
        "esbuild": "^0.12.6",
        "google-spreadsheet": "^3.0.13",
        "js-yaml": "^3.14.0",
        "netlify-cms": "^2.10.137",
        "netlify-cms-proxy-server": "^1.3.18",
        "netlify-lambda": "^2.0.8",
        "npm-run-all": "^4.1.5",
        "parcel-bundler": "^1.12.5",
        "post-cli": "^1.0.5",
        "postcss-preset-env": "^6.7.0",
        "toml": "^3.0.0",
        "typescript": "^4.0.3"
    }
}
